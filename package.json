{
  "name": "app-in-the-air",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "json-server --watch src/api/db.json --port 5001",
    "prettier": "prettier -w \"src/**/*.tsx\"",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "build-storybook": "storybook build -c ./config/storybookn"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:@tanstack/eslint-plugin-query/recommended",
      "plugin:storybook/recommended"
    ],
    "rules": {
      "no-multiple-empty-lines": [
        "warn",
        {
          "max": 1
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always",
        {
          "omitLastInOneLineBlock": true
        }
      ]
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none",
    "endOfLine": "auto"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.1.35",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "redux-persist": "^6.0.0",
    "sass": "^1.62.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.23",
    "@storybook/addon-interactions": "^7.0.23",
    "@storybook/addon-links": "^7.0.23",
    "@storybook/blocks": "^7.0.23",
    "@storybook/preset-create-react-app": "^7.0.23",
    "@storybook/react": "^7.0.23",
    "@storybook/react-webpack5": "^7.0.23",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@tanstack/eslint-plugin-query": "^4.29.8",
    "@types/google.maps": "^3.53.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.19",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-plugin-storybook": "^0.6.12",
    "json-server": "^0.17.3",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.23",
    "storybook-addon-react-router-v6": "^1.0.2",
    "webpack": "^5.88.0"
  }
}
